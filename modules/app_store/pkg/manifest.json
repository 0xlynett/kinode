[
    {
        "process_name": "main",
        "process_wasm_path": "/app_store.wasm",
        "on_exit": "Restart",
        "request_networking": true,
        "request_capabilities": [
            "terminal:terminal:nectar",
            "filesystem:sys:nectar",
            "http_server:sys:nectar",
            "http_client:sys:nectar",
            "net:sys:nectar",
            "vfs:sys:nectar",
            "kernel:sys:nectar",
            "eth:sys:nectar",
            {
                "process": "vfs:sys:nectar",
                "params": {
                    "root": true
                }
            }
        ],
        "grant_capabilities": [
            "http_server:sys:nectar",
            "terminal:terminal:nectar",
            "vfs:sys:nectar"
        ],
        "public": false
    }
]